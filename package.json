{
  "name": "fjl-validator",
  "version": "0.6.17",
  "description": "Functional validators for forms and the like.\"",
  "main": "dist/cjs/fjlValidator.js",
  "module": "dist/es6-module/fjl-validator.js",
  "repository": "git@github.com:functional-jslib/fjl-validator.git",
  "author": "Ely De La Cruz <elycruz@elycruz.com>",
  "license": "BSD",
  "scripts": {
    "pre-publish": "npm test && npm run build && npm run docs",
    "prepub": "npm run pre-publish",
    "build": "gulp clean && gulp build && npm run test-builds",
    "docs": "gulp jsdoc",
    "test-builds": "jest --testMatch **/tests/*-test-*.js",
    "test": "jest --testMatch **/tests/test-*.js"
  },
  "dependencies": {
    "fjl-mutable": "^0.16.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "graceful-fs": "^4.1.11",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-better-rollup": "^2.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^4.0.0",
    "gulp-fncallback": "^2.0.2",
    "gulp-header": "^1.8.9",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-uglify": "^3.0.0",
    "gulp-uglify-es": "^0.1.9",
    "jest": "^21.2.1",
    "lazypipe": "^1.0.1",
    "minimatch": "^3.0.4",
    "rollup": "^0.52.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "yargs": "^10.0.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "buildConfig": {
    "inputModuleName": "fjlValidator",
    "inputFilePath": "./src/fjlValidator.js",
    "outputFileName": "fjl-validator.js",
    "outputFileNameMin": "fjl-validator.min.js",
    "srcsGlob": "./src/**/*.js",
    "paths": {
      "dist": "./dist",
      "docs": "./docs"
    },
    "folderNames": {
      "amd": "amd",
      "cjs": "cjs",
      "iife": "iife",
      "umd": "umd",
      "es6Module": "es6-module"
    },
    "es6ModuleRollup": {
      "config": {
        "input": "./src/fjlValidator.js",
        "external": [
          "fjl",
          "fjl-mutable"
        ]
      },
      "write": {
        "file": "./dist/es6-module/fjl-validator.js",
        "format": "es",
        "sourcemap": true
      }
    },
    "amdRollup": {
      "babelPlugin": {
        "babelrc": false,
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          "transform-es2015-modules-amd"
        ]
      }
    },
    "cjsRollup": {
      "babelPlugin": {
        "babelrc": false,
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          "babel-plugin-add-module-exports"
        ]
      }
    },
    "iifeRollup": {
      "babelPlugin": {
        "babelrc": false,
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          "external-helpers"
        ]
      }
    },
    "umdRollup": {
      "babelPlugin": {
        "babelrc": false,
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          "babel-plugin-add-module-exports",
          "transform-es2015-modules-umd"
        ]
      }
    }
  }
}
